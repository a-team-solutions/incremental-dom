{
  "compilerOptions": {
    "outDir": "dist/",
    "allowSyntheticDefaultImports": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "pretty": true,
    "strict": true,
    "target": "es5",
    "lib": ["es2015", "dom"]
  },
  "include": [
    "index.ts",
    "src/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
